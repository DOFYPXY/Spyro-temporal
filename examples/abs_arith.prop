//@Description Toy benchmarks to show complex recursive generators.

var {
    int x;
    int o;
}

relation {
    abs(x, o);
}

generator {
    boolean B -> I < 0 | I <= 0 | I == 0 | I != 0 ;
    int I -> C * x + C * o + C ;
    int C -> ??(2) - 1 ;
}

example {
    int -> ?? | -1 * ?? ;
}

void abs(int x, ref int out){
    if (x < 0) {
        out = -x;
    } else {
        out = x;
    }
}