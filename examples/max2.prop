//@Description Toy benchmarks to show complex recursive generators.

var {
    int x;
    int y;
    int o;
}

relation {
    max2(x, y, o);
}

generator {
    boolean B -> I < I | I == I ;
    int I -> x | y | o ;
}

example {
    int -> ??(5) | -1 * ??(5) ;
}

void max2(int x, int y, ref int out){
    if (x > y) {
        out = x;
    } else {
        out = y;
    }
}