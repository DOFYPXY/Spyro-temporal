//@Description Toy benchmarks to show complex recursive generators.

var {
    int x;
    int o;
}

relation {
    sum(x, o);
}

generator {
    boolean AP -> E < C | E > C | E == C | E != C ;
    int E -> C * x + C * o + C * x * x;
    int C -> ??(3) - 4  ;
}

example {
    int -> ?? | -1 * ?? ;
}

void sum(int x, ref int out){
    if (x > 0) {
        int sub_out;
        sum(x-1, sub_out);
        out = sub_out + x;
    } else {
        out = 0;
    }
}